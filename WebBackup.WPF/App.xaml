<Application x:Class="WebBackup.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WebBackup.WPF"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/Fonts/Fonts.xaml" />
                
                <local:SkinResourceDictionary SkinName="Dark" Source="/Resources/Themes/DarkTheme.xaml" />
                <local:SkinResourceDictionary SkinName="Default" Source="/Resources/Themes/DefaultTheme.xaml" />
                
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
