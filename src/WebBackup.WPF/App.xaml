<Application x:Class="WebBackup.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WebBackup.WPF"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!--Fonts, string locales-->
                <ResourceDictionary Source="/Resources/Fonts/Fonts.xaml" />
                <ResourceDictionary Source="/Resources/Fonts/FontIcons.xaml" />
                <ResourceDictionary Source="/Resources/Languages/Strings.en-US.xaml" />
                <!--Skins, themes-->
                <local:SkinResourceDictionary SkinName="Dark" Source="/Resources/Skins/Dark.xaml" />
                <local:SkinResourceDictionary SkinName="Default" Source="/Resources/Skins/Default.xaml" />
                <!--Templates for Controls, Windows-->
                <ResourceDictionary Source="/Resources/Templates/Validations.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
